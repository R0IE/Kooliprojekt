@model KooliProjekt.Models.PagedResult<KooliProjekt.Data.TeamMembers>

@{
    ViewData["Title"] = "Team Members";
}

<h1>Team Members</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>User</th>
            <th>Project</th>
        </tr>
    </thead>
    <tbody>
    @if (Model?.Results != null)
    {
        foreach (var item in Model.Results)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserId</td>
                <td>@item.ProjectId</td>
            </tr>
        }
    }
    </tbody>
</table>

<div>
    @await Component.InvokeAsync("Pager", new { model = Model })
</div>
